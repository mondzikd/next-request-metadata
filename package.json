{
  "name": "next-request-metadata",
  "version": "2.0.0",
  "description": "Next.js SSR request metadata context",
  "author": "Dominik Mondzik",
  "engines": {
    "node": ">=v16.4.0"
  },
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "TS_NODE_TRANSPILE_ONLY=1 node --loader ts-node/esm --test 'test/**/*.test.ts'",
    "build": "tsc -p tsconfig.json",
    "style:check": "prettier --check .",
    "style:fix": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.55.0"
  },
  "type": "module",
  "keywords": [
    "Next.js request logs",
    "x-request-id",
    "Log middleware",
    "Log metadata",
    "Log context"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mondzikd/next-request-metadata.git"
  },
  "bugs": "https://github.com/mondzikd/next-request-metadata",
  "license": "ISC"
}
